clear all
clc
close all

r = 3.8285;
for j = 1:10000
    x(j) = j/10000;
    y(j) = r*x(j)*(1-x(j));
    z(j) = r*y(j)*(1-y(j));
    w(j) = r*z(j)*(1-z(j));
end
 
x2 = linspace(0,1,10000);
y2 = x2;

x3 = [0.7388];
y3 = x3;

x4 = [0.1603,0.515252,0.956296];
y4 = x4;

plot(x,w,x2,y2);
hold on
scatter(x3,y3,'filled','MarkerFaceColor',[1 0 0]);
hold on
scatter(x4,y4,'filled','MarkerFaceColor',[0 0.5 0]);
ylabel('x_{n+3}', 'FontSize', 20);
xlabel('x_n', 'FontSize', 20);
